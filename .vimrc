" colorscheme monokai-phoenix
filetype plugin on
set autoindent
set clipboard=unnamedplus
set endofline
set expandtab
set foldlevelstart=10
set foldmethod=syntax
set hlsearch
set incsearch
set laststatus=2
set list
set listchars=space:·,tab:»·
set mouse=a
set number
set ruler
set shiftwidth=2
set showmatch
set tabstop=2
set wrap
syntax on
